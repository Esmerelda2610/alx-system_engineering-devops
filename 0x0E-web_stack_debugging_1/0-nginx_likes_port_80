#usr/bin/env bash
Fixes and configures Nginx to listen on port 80

# Remove the default Nginx configuration file
rm /etc/mginx/sites-enabled/default
# Configure Nginx to listen on port 80 for all active Ipvs and Ips
In -s /etc/nginx/sites-available/default etc/nginx/sites-enabled/default
#Restart Nginx
service nginx restart
